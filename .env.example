# Protect your endpoint (any long random string)
AUTH_TOKEN=

# Persistent user-data dir (MUST live on a mounted disk)
USER_DATA_DIR=./data/user-data

# Page part of the URL (the "fb..." segment in your Manychat URLs)
MC_PAGE_ID=

# Port for the web server
PORT=3000

# Headless mode; set to false ONLY for the one-time OAuth login
HEADLESS=true
